<h1>Listing api_vuelos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_vuelos.each do |api_vuelo| %>
      <tr>
        <td><%= link_to 'Show', api_vuelo %></td>
        <td><%= link_to 'Edit', edit_api_vuelo_path(api_vuelo) %></td>
        <td><%= link_to 'Destroy', api_vuelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vuelo', new_api_vuelo_path %>
